{
  "name": "jobzonme",
  "version": "1.0.0",
  "description": "Web app that allows to track job application of a client",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build-client": "cd client && npm run build",
    "server": "nodemon server --ignore client",
    "client": "npm start --prefix client",
    "dev": "concurrently --kill-others-on-fail \" npm run server\" \" npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muratistek/JobzOnMe.git"
  },
  "keywords": [
    "mern"
  ],
  "author": "Murat Istek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/muratistek/JobzOnMe/issues"
  },
  "homepage": "https://github.com/muratistek/JobzOnMe#readme",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^6.10.0",
    "morgan": "^1.10.0",
    "validator": "^13.9.0",
    "xss-clean": "^0.1.1"
  }
}
